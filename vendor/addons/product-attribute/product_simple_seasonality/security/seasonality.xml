<odoo>

    <record model="ir.model.access" id="campaign_seasonality_access_settings">
        <field name="name">Seasonality config</field>
        <field name="model_id" ref="model_seasonality" />
        <field name="group_id" ref="base.group_system" />
        <field name="perm_read" eval="1" />
        <field name="perm_create" eval="1" />
        <field name="perm_write" eval="1" />
        <field name="perm_unlink" eval="1" />
    </record>

    <record model="ir.model.access" id="campaign_seasonality_access_user">
        <field name="name">Seasonality user</field>
        <field name="model_id" ref="model_seasonality" />
        <field name="group_id" ref="base.group_user" />
        <field name="perm_read" eval="1" />
        <field name="perm_create" eval="0" />
        <field name="perm_write" eval="0" />
        <field name="perm_unlink" eval="0" />
    </record>

</odoo>
